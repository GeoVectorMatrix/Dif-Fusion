{
    "name": "ddpm-fusion-Unet",
    "phase": "train",
    "distributed": false,
    "gpu_ids": [
        0
    ],
    "path": {
        "log": "logs",
        "tb_logger": "tb_logger",
        "results": "results",
        "checkpoint": "checkpoint",
        "resume_state": null
    },
    "datasets": {
        "train": {
            "name": "MSRS",
            "dataroot":  "TBD" ,
            "resolution": 160,
            "batch_size": 1,
            "num_workers": 1,
            "use_shuffle": true,
            "data_len": -1
        },
        "val": {
            "name": "MSRS",
            "resolution": 160,
            "image_size_w": 160,
            "image_size_h": 160,
            "data_len": 1
        }
    },
    "model": {
        "which_model_G": "sr3",
        "finetune_norm": false,
        "unet": {
            "in_channel": 4,
            "out_channel": 4,
            "inner_channel": 128,
            "channel_multiplier": [
                1,
                2,
                4,
                8,
                8
            ],
            "attn_res": [
                16
            ],
            "res_blocks": 2,
            "dropout": 0.2
        },
        "beta_schedule": {
            "train": {
                "schedule": "cosine",
                "n_timestep": 2000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            },
            "val": {
                "schedule": "cosine",
                "n_timestep": 2000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            }
        },
        "diffusion": {
            "image_size": 160,
            "image_size_w": 160,
            "image_size_h": 160,
            "is_concat": false,
            "channels": 4,
            "loss": "l2",
            "conditional": false
        }
    },
    "train": {
        "n_iter": 1000000,
        "val_freq": 5000,
        "save_checkpoint_freq": 1e4,
        "print_freq": 200,
        "optimizer": {
            "type": "adamw",
            "lr": 1e-5
        }
    }
}
